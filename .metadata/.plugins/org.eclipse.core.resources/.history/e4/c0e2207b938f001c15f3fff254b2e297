   Description : 배열(Array) 복사
 */
package Java0217;

public class ex03_ArrayCopy {

	public static void main(String[] args) {
		// 
		
		String fire1 [] = {"강병근", "고윤섭","김가연","김유경","김준호"}
		String fire2 [] = new String[5];
		
		System.arraycopy(fire1, 0, fire2, 0, 0);;

  }

}
