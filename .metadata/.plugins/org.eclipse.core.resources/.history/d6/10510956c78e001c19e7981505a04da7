package Java0216;

public class ex02_starEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		
		for(int i; i <= 5; i++) {
			
			for(int  j; j<= i; j++) {
				
				System.out.println("*");
			}
			
		}
		
	}

}
