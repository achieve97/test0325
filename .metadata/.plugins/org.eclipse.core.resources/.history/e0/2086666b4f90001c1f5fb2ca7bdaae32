package java0217;

public class ex06_arrayEx4 {

	public static void main(String[] args) {
		
		// 2차원 배열과
		// 중첩for문 사용해서
		// 구구단 출력하기!
		
		// 2~9단 : 8개(i)
		// x1 ~ x9 : 9개(j)
		
		// int multi[][] = new int[10][10];		
		int multi[][] = new int[8][9];
		
//		System.out.println(multi.length);
//		System.out.println(multi[0].length);
		
		for(int i=2; i<multi.length+2; i++) {
			System.out.println("==== "+ i + "단 ====");
			for(int j=1; j<multi[0].length+1; j++) {
				multi[i-2][j-1] = i * j;
				
				System.out.println(i + " x "+ j +" =  " + multi[i-2][j-1]);
			}
			System.out.println();
		}
		
		

	}

}
