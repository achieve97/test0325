package Java0217;

public class ex06_arrayEx4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 2차원 배열과 
		// 중첩 for문 사용해서
		// 구구단 출력하기
		
		for(int i= 2; i<=9; i++) {
			
			for(int j= 1; j<=9; j++) {
				System.out.println(i + "x" + j + "= " + i*j);
			}
			
		}
	}

}
