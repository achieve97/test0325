package Java0215;

public class ex08_loopFor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		    // 10번 출력
		
		for(int i = 1; i<=10 ; i++) {
			
			System.out.println(i+"실행문");
		}
		
		
		
		
		
	}

}
