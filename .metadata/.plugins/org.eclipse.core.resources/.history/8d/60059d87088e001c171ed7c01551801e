package Java0215;

public class ex03_condition {
	public static void main(String[]args) {
		
		boolean condition = true;
		if(condition) {
			System.out.println("조건 만족!");
			System.out.println("condition : " + condition);
		} else {
			System.out.println("조건 불만족!");
			System.out.println("condition : " + condition);
		}
	}
}
